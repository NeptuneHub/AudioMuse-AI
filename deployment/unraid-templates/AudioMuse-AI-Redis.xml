<?xml version="1.0"?>
<Container version="2">
  <Name>AudioMuse-AI-Redis</Name>
  <Repository>redis:7-alpine</Repository>
  <Registry>https://hub.docker.com/_/redis</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/NeptuneHub/AudioMuse-AI/discussions</Support>
  <Project>https://github.com/NeptuneHub/AudioMuse-AI</Project>
  <Overview>Redis instance dedicated to AudioMuse-AI. Redis is used as a message queue for background jobs (RQ - Redis Queue).&#xD;
&#xD;
This container is required for AudioMuse-AI to function properly.&#xD;
&#xD;
Install this container BEFORE installing AudioMuse-AI.</Overview>
  <Category>Tools: MediaApp:Music</Category>
  <WebUI/>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/docker-library/docs/01c12653951b2fe592c1f93a13b4e289ada0e3a1/redis/logo.png</Icon>
  <ExtraParams>--restart=unless-stopped</ExtraParams>
  <PostArgs/>
  <CPUset/>
  <DateInstalled/>
  <DonateText/>
  <DonateLink/>
  <Requires>Part of AudioMuse-AI stack</Requires>
  <Config Name="Redis Port" Target="6379" Default="6380" Mode="tcp" Description="Redis port (6380 to avoid conflict with other Redis instances)" Type="Port" Display="always" Required="true" Mask="false">6380</Config>
  <Config Name="Redis Data" Target="/data" Default="/mnt/user/appdata/audiomuse-ai/redis" Mode="rw" Description="Redis persistent data" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/audiomuse-ai/redis</Config>
</Container>